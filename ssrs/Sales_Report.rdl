    <DataSet Name="MonthlySales">
      <Query>
        <DataSourceName>DataVizDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    ShowroomCity,
    YearMonth,
    MonthlyRevenue
FROM dbo.vMonthlySalesByShowroom
WHERE 
    (@StartDate IS NULL OR CAST(YearMonth + '-01' AS date) &gt;= @StartDate)
AND (@EndDate IS NULL OR CAST(YearMonth + '-01' AS date) &lt;= @EndDate)
ORDER BY YearMonth, ShowroomCity</CommandText>
      </Query>
      <Fields>
        <Field Name="ShowroomCity">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ShowroomCity</DataField>
        </Field>
        <Field Name="YearMonth">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>YearMonth</DataField>
        </Field>
        <Field Name="MonthlyRevenue">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>MonthlyRevenue</DataField>
        </Field>
      </Fields>
    </DataSet>
