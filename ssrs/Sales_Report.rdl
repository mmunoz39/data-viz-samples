  <DataSets>
    <DataSet Name="SalesDetail">
      <Query>
        <DataSourceName>DataVizDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    OrderDate,
    ShowroomCity,
    CustomerName,
    Segment,
    CustomerCity,
    CustomerProvince,
    ProductName,
    Category,
    Quantity,
    UnitPrice,
    LineTotal
FROM dbo.vSalesDetail
WHERE (@StartDate IS NULL OR OrderDate &gt;= @StartDate)
AND (@EndDate IS NULL OR OrderDate &lt;= @EndDate)
ORDER BY OrderDate</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderDate">
          <rd:TypeName>System.DateTime</rd:TypeName>
